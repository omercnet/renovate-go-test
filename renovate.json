{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "postUpdateOptions": ["gomodTidy", "gomodUpdateImportPaths"],
  "env:": {
    "GOPRIVATE": "github.com/omercnet/*"
  },
  "packageRules": [
    {
      "packagePatterns": ["omercnet"],
      "groupName": "Test untagged",
      "digest": {
        "enabled": true
      },
      "pinDigests": true
    }
  ]
}
